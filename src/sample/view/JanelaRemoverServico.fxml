<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.text.Text?>
<BorderPane stylesheets="estilo.css" fx:id="TELA" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.control.ControlRemoverServico"
            prefHeight="400.0" prefWidth="600.0">


    <top >
        <HBox alignment="CENTER" >
            <Text fx:id="txtLogo" text="Remover Serviço" ></Text>
        </HBox>
    </top>
    <center>
        <GridPane alignment="CENTER">
            <VBox alignment="CENTER" spacing="10">
                <JFXComboBox promptText="Serviço" GridPane.rowIndex="0" GridPane.columnIndex="0" fx:id="cbServico" ></JFXComboBox>
                <HBox alignment="CENTER" spacing="10">
                    <JFXButton GridPane.rowIndex="1" GridPane.columnIndex="0" onAction="#Confirmar" text="Remover!"></JFXButton>
                    <JFXButton onAction="#Voltar" text="Voltar"></JFXButton>
                </HBox>
            </VBox>
        </GridPane>
    </center>
</BorderPane>
